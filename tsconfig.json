{
  "compilerOptions": {
      "module": "commonjs",
      "lib": [ "es2015", "dom" ],
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      // "outFile": "www/js/app.js",
      "outDir": "www/js",
      "sourceMap": true,
      "strict": true
  },
  "include": [
      "src/**/*.ts"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}